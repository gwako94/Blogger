<h1> All article </h1>
<% if logged_in? %>
<ul id="articles">
  <% @articles.each do |article| %>
    <li>
      <%= link_to article.title, article_path(article), class: 'article_title' %>
    </li>
  <% end %>
  <%= link_to "Create artcle", new_article_path, class: 'new_article' %>
</ul>
<% end %>
